{
  "name": "ng-cash-server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --exit-child --watch src src/index.ts",
    "build": "rimraf dist && swc ./src -d dist",
    "start": "node dist/index.js",
    "start:docker": "yarn build && yarn start",
    "rebuild:api": "docker compose build api && docker compose rm --stop api && docker compose up -d api",
    "db:console": "docker compose exec postgres psql -h localhost -U docker -d ng_cash_dev",
    "db:migrate": "yarn prisma migrate dev",
    "db:reset": "yarn prisma migrate reset",
    "docker:db:migrate": "docker compose exec api yarn db:migrate",
    "docker:db:reset": "docker compose exec api yarn db:reset",
    "docker:db:seed": "docker compose exec api yarn prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "4.6.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.18",
    "@tsconfig/node-lts": "^18.12.1",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "prisma": "^4.6.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0"
  }
}
